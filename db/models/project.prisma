model Project {
  id          String  @id @default(cuid()) @map("project_id")
  title       String
  description String?
  repoUrl     String? @map("repo_url")
  liveUrl     String? @map("live_url")

  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  @@map("projects")
}
